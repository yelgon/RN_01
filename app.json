{
  "expo": {
    "name": "Mortgage and Annuity calculator",
    "slug": "myapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "package": "com.yelgon.cal",
      "versionCode": 4,
      "permissions": ["default"]
    },
    "ios": {
      "bundleIdentifier": "com.yelgon.cal"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
